{
  "name": "proj4-cli-defs",
  "version": "2.0.2",
  "description": "A Node.js cli to generate one easy-to-use file for proj4js of all epsg definitions from PostGIS ",
  "main": "index.js",
  "access": "public",
  "private": false,
  "scripts": {
    "build": "tsc",
    "generate": "node scripts/generate.ts",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build"
  },
  "type": "module",
  "author": "Zoran Kokeza",
  "license": "MIT",
  "peerDependencies": {
    "proj4": "^2.3.12"
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "@types/pg": "^8.16.0",
    "commander": "^14.0.3",
    "pg": "^8.18.0",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3"
  },
  "exports": {
    "./espg.js": {
      "import": "./dist/epsg.js",
      "types": "./dist/epsg.d.ts"
    },
    "./espg-fn.js": {
      "import": "./dist/epsg-fn.js",
      "types": "./dist/epsg-fn.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zoran995/proj4-cli"
  },
  "bugs": {
    "url": "https://github.com/zoran995/proj4-cli/issues"
  }
}
